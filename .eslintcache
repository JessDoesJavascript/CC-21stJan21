te({\n                popUpContent: {\n                    title: \"Brownies\",\n                    info: \"Info about brownies!\",\n                    image: \"Image placeholder\"\n                }\n            })\n        } else if (s === 'cakes') {\n            this.setState({\n                popUpContent: {\n                    title: \"Cakes\",\n                    info: \"Info about cakes\",\n                    image: \"Image placeholder\"\n                }\n            })\n        } else if (s === 'cupcakes') {\n            this.setState({\n                popUpContent: {\n                    title: \"Cupcakes\",\n                    info: \"Info about cupcakes\",\n                    image: \"Image placeholder\"\n                }\n            })\n        } else if (s === 'tiered-cakes') {\n            this.setState({\n                popUpContent: {\n                    title: \"Tiered cakes\",\n                    info: \"Info about tiered cakes\",\n                    image: \"Image placeholder\"\n                }\n            })\n        } \n        this.setState({\n            popUpShown: !this.state.popUpShown,\n        })\n    }\n\n    doNothing = (e) => {\n        e.preventDefault();\n        e.stopPropagation();\n    }\n\n\n    render() {\n        return (\n            <StyledDiv> \n            <a href=\"menu\" id=\"menu\" />\n                <StyledH3> the menu </StyledH3>\n                {this.state.popUpShown === false ? null : <MenuPopUp click={this.clickHandler} \n                                                                 doNothingClick={this.doNothing} \n                                                                 title={this.state.popUpContent.title} \n                                                                 info={this.state.popUpContent.info} \n                                                                 image={this.state.popUpContent.image}/>}\n                <StyledImagesContainer> \n                {/* Brownies image and button */}\n                    <StyledImgCard>\n                        <StyledImg src={brownies} \n                                   alt='Chocolate oreo brownies.'\n                                   onClick={() => {this.clickHandler('brownies')}}>                         \n                        </StyledImg>\n                        <StyledButton onClick={() => {this.clickHandler('brownies')}}>Brownies</StyledButton>\n                    </StyledImgCard>\n\n                {/* Cakes image and button  */}\n                    <StyledImgCard>\n                        <StyledImg src={chocolateCake} \n                                   alt='Chocolate cake decorated with various chocolate bars and sweets'\n                                   onClick={() => {this.clickHandler('cakes')}}>\n                        </StyledImg>\n                        <StyledButton onClick={() => {this.clickHandler('cakes')}}>Cakes</StyledButton>\n                    </StyledImgCard>\n\n                {/* Cupcakes image and button  */}\n                    <StyledImgCard>\n                        <StyledImg src={cupcakes} \n                                   alt='Cupcakes decorated with buttercream and slices of mars bar.'\n                                   onClick={() => {this.clickHandler('cupcakes')}}>\n                        </StyledImg>\n                        <StyledButton onClick={() => {this.clickHandler('cupcakes')}}>Cupcakes</StyledButton>\n                    </StyledImgCard>\n                \n                {/* Tiered cakes  */}\n                    <StyledImgCard>  \n                        <StyledImg src={tieredCake} \n                                   alt='Beautiful 3-tiered wedding cake decorated in buttercream and pale pink roses.'\n                                   onClick={() => {this.clickHandler('tiered-cakes')}}>\n                        </StyledImg>\n                        <StyledButton onClick={() => {this.clickHandler('tiered-cakes')}}>Tiered Cakes</StyledButton>\n                    </StyledImgCard>\n                </StyledImagesContainer>\n            </StyledDiv>\n        )\n    }\n}\n\nexport default Menu;\n","/Users/jessiechapman/Documents/Coding/cc-21stjan21/src/components/Gallery.js",["91"],"/Users/jessiechapman/Documents/Coding/cc-21stjan21/src/components/GalleryImage.js",[],"/Users/jessiechapman/Documents/Coding/cc-21stjan21/src/components/Testimonials.js",["92"],"import React from 'react';\nimport styled from 'styled-components';\nimport ReviewCard from './ReviewCard';\n\n\nconst StyledDiv = styled.div`\n    width: 100vw;\n    background-color: ${props => props.theme.colors.palePink};\n    display: flex;\n    flex-direction: column; \n    justify-content: center;\n    align-items: center;\n\n`; \n\nconst StyledH3 = styled.h3`\n    color: ${props => props.theme.colors.darkPurple};\n    font-family: ${props => props.theme.fonts.fancy};\n    font-weight: 600;\n    font-size: 40px; \n    letter-spacing: 2px; \n    margin: 10px; \n    padding: 10px; \n`;\n\nconst StyledReviewCardsContainer = styled.div`\n    display: flex;\n    align-items: center;\n    justify-content: center; \n`;\n\n\nfunction Testimonials() {\n    return (\n        <StyledDiv> \n        <a id=\"testimonials\" href=\"testimonials\" />\n            <StyledH3>testimonials</StyledH3>\n            <StyledReviewCardsContainer>\n                <ReviewCard content=\"The cake looked beautiful and tasted even better!\"></ReviewCard>\n                <ReviewCard content=\"The cake looked beautiful and tasted even better!\"></ReviewCard>\n                <ReviewCard content=\"The cake looked beautiful and tasted even better!\"></ReviewCard>\n            </StyledReviewCardsContainer>\n\n        </StyledDiv>\n    )\n}\n\nexport default Testimonials;","/Users/jessiechapman/Documents/Coding/cc-21stjan21/src/components/ReviewCard.js",[],"/Users/jessiechapman/Documents/Coding/cc-21stjan21/src/components/ContactForm.js",["93","94"],"import R